<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<fx:root fx:id="border_pane" stylesheets="@app.css" type="BorderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <MenuBar>
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <ToolBar>
              <items>
                  <Button fx:id="saveButton" mnemonicParsing="false" text="Save" />
                  <Button fx:id="generateButton" mnemonicParsing="false" text="Generate" />
                  <Button fx:id="exportButton" mnemonicParsing="false" text="Export" />
                  <Button fx:id="clearButton" mnemonicParsing="false" text="Clear" />
              </items>
            </ToolBar>
         </children>
      </VBox>
   </top>
   <right>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <TitledPane fx:id="configBox" animated="false" text="Configuration">
              <content>
                <AnchorPane fx:id="configPane" prefHeight="219.0" prefWidth="160.0" />
              </content>
            </TitledPane>
            <TitledPane fx:id="applicationsList" animated="false" prefHeight="267.0" prefWidth="162.0" text="Applications">
               <content>
                  <SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="256.0" prefWidth="160.0">
                    <items>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                           <children>
                              <ScrollPane prefHeight="70.0" prefWidth="160.0" />
                           </children>
                        </AnchorPane>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="13.0" prefWidth="160.0" SplitPane.resizableWithParent="false">
                           <children>
                              <HBox fillHeight="false" prefHeight="65.0" prefWidth="160.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="-10.0">
                                 <children>
                                    <Button fx:id="createAppButton" mnemonicParsing="false" text="Create Application" HBox.hgrow="NEVER">
                                       <effect>
                                          <Glow />
                                       </effect>
                                       <HBox.margin>
                                          <Insets bottom="8.0" left="10.0" top="20.0" />
                                       </HBox.margin>
                                       <padding>
                                          <Insets bottom="8.0" left="10.0" right="8.0" top="8.0" />
                                       </padding>
                                    </Button>
                                 </children>
                              </HBox>
                           </children>
                        </AnchorPane>
                    </items>
                  </SplitPane>
               </content>
            </TitledPane>
         </children>
      </VBox>
   </right>
   <bottom>
      <TextArea fx:id="textArea" prefHeight="99.0" prefWidth="640.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </padding>
      </TextArea>
   </bottom>
   <center>
      <SplitPane fx:id="base_pane" dividerPositions="0.2" prefHeight="0.0" prefWidth="0.0" BorderPane.alignment="CENTER">
        <items>
            <ScrollPane fitToHeight="true" prefViewportHeight="384.0" prefViewportWidth="169.0" vbarPolicy="NEVER">
               <content>
                  <VBox fx:id="left_pane" spacing="10.0">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </VBox>
               </content>
            </ScrollPane>
          <AnchorPane fx:id="right_pane" prefHeight="317.0" prefWidth="423.0" />
        </items>
      </SplitPane>
   </center>
</fx:root>
